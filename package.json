  "version": "1.0.0",
  "scripts": {
    "lint": "prettier ./",
    "lint:fix": "npm run lint -- --write",
    "simulate:mainnet": "env-cmd aave-tenderly-cli --networkId 1 --keepAlive --artifactPath",
